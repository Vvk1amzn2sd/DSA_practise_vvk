{
  "hosting": {
    "public": "public",
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      "dsabackend/**",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.exe",
      "*.bin",
      "*.sh"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
